<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.dante.tammodertates">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/launcher_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".Social_Media"
                android:label="@string/social_media_label">
        </activity>
        <activity
                android:name=".Meet_The_Officers"
                android:label="@string/title_activity_meet_the_officers"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".About"
                android:label="@string/app_name">
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>

        <activity android:name=".Important_Dates"
                  android:label="@string/tamm_important_dates_title">
        </activity>
    </application>

</manifest>